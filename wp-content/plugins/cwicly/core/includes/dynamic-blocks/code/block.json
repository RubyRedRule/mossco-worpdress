{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "cwicly/code",
    "title": "Code",
    "category": "cwicly",
    "description": "Execute your own code (PHP, JS, HTML and CSS) directly from a block, and instantly preview the result.",
    "keywords": [
        "code",
        "php",
        "js",
        "css",
        "editor",
        "cwicly"
    ],
    "textdomain": "cwicly",
    "supports": {
        "customClassName": false,
        "html": false
    },
    "usesContext": [
        "cart_item",
        "cart_key",
        "filterInfo",
        "filterType",
        "postId",
        "product",
        "query",
        "queryId",
        "query_index",
        "repeater_row",
        "repeaters",
        "taxterms",
        "taxterms_index",
        "termQuery",
        "userQuery",
        "commentQuery",
        "wooVariable",
        "queryTotal",
        "queryPageKey",
        "paginateArgs",
        "queryInherit",
        "has_parent",
        "rendered",
        "hasPosts"
    ],
    "attributes": {
        "isStyling": {
            "type": "boolean"
        },
        "forceShowID": {
            "type": "boolean"
        },
        "forceShowClass": {
            "type": "boolean"
        },
        "editorHidden": {
            "type": "boolean"
        },
        "skeletonActive": {
            "type": "boolean",
            "default": true
        },
        "skeletonSizeWidth": {
            "type": "object"
        },
        "skeletonSizeHeight": {
            "type": "object"
        },
        "skeletonBorderRadius": {
            "type": "object"
        },
        "executeCode": {
            "type": "boolean"
        },
        "showBlock": {
            "type": "boolean",
            "default": true
        },
        "code": {
            "type": "string"
        },
        "codePreventClicks": {
            "type": "boolean"
        },
        "codeRender": {
            "type": "string"
        },
        "codeCSS": {
            "type": "string"
        },
        "codeJS": {
            "type": "string"
        },
        "pseudoContent": {
            "type": "object"
        },
        "anchor": {
            "type": "string"
        },
        "pseudoClasses": {
            "type": "array"
        },
        "uniqueID": {
            "type": "string"
        },
        "hideLoggedIn": {
            "type": "boolean"
        },
        "hideGuest": {
            "type": "boolean"
        },
        "hideConditions": {
            "type": "array",
            "default": []
        },
        "hideConditionsType": {
            "type": "string",
            "default": "&&"
        },
        "classID": {
            "type": "string"
        },
        "id": {
            "type": "string"
        }
    }
}